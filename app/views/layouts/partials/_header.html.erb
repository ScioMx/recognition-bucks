<header class="navbar navbar-fixed-top">
  <div class="navbar-inner">
    <div class="container">
      <%=  link_to "Logo goes here!", static_pages_home_path, id: "logo" %>
      <nav> 
        <ul class="nav pull-right">
          <li><%= link_to "Home", static_pages_home_path %></li>
          <li><%= link_to "Help", static_pages_help_path %></li>
          <% if current_user %>
            <li><%= link_to "Logged in as " + current_user.email, "#" %> </li>
            <li><%= link_to "Log out", destroy_user_session_path, :method => :delete, :id => "lnk_log_out" %></li>
            <% if admin? %>
              <li><%= link_to 'Manage users', admin_users_path %></li>
            <% end %>
          <% else %>
            <li><%= link_to "Log in", new_user_session_path, :id => "lnk_log_in" %></li>
            <li><%= link_to "Sign up", new_user_registration_path, :id => "lnk_sign_up" %></li>
          <% end %>
        </ul>
      </nav>
    </div>
      </div>
</header>