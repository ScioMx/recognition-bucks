<header class="navbar navbar-fixed-top">
  <div class="navbar-inner">
    <div class="container">
      <%=  link_to "Logo goes here!", "#", id: "logo" %>
      <nav> 
        <ul class="nav pull-right">
          <li><%= link_to "Home", static_pages_home_path %></li>
          <li><%= link_to "Help", static_pages_help_path %></li>
          <% if current_user %>
            Logged in as <%= current_user.name %>
            <%= link_to "Log out", log_out_path, :id => "lnk_log_out" %>
          <% else %>
            <li><%= link_to "Log in ", log_in_path, :id => "lnk_log_in" %>/</li>
            <li><%= link_to "Sign up", sign_up_path, :id => "lnk_sign_up" %></li>
          <% end %>
        </ul>
      </nav>
    </div>
      </div>
</header>